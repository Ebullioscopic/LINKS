<Loading_Screen>:
    name: "Loader"
    BoxLayout:
        orientation: "vertical"
        Label:
            id: bell
            Image:
                source: "sparkling_infinity_loader.gif"
                anim_delay: 0.05
                size: bell.size
                pos: bell.pos
                allow_stretch: True
                keep_ratio: True
        #Label:
        #    text: "Please wait until ContactApp starts"
            #height: 0
        #    size_hint_y: None
        #    height: 40
        #    size_hint_x: 0.3
<Filer>:
    id: filit
    title: "Attach"
    title_size: 20
    separator_height: 0
    size_hint: None, None
    size: 500, 500
    auto_dismiss: False
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        FileChooserIconView:
            id: filechoosey
        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: filit.dismiss()#root.cancel()
            Button:
                text: "Load"
                on_release: root.load(filechoosey.selection)    

<WhatsAppSender>:
    title:"WABot"
    title_size: 40
    title_color: utils.get_color_from_hex("#008000")
    title_align: "center"
    separator_height: 0
    size_hint: None, None
    size: 500, 500
    background: "whitest.png"
    auto_dismiss: False
    BoxLayout:
        orientation: "vertical"
        Label:
            id: wa
            Image:
                source: 'icons8-whatsapp.png'
                allow_stretch: True
                keep_ratio: True
                size: wa.size
                pos: wa.pos            
        GridLayout:
            cols: 2
            TextInput:
                hint_text: "Message"
                id: message_text
            Button:
                id: whatsapp_pin
                on_press: Factory.Filer().open()
                size_hint: None, None
                size: 50, 45
                Image:
                    source: 'icons8-attach-96.png'
                    allow_stretch: True
                    keep_ratio: True
                    size: whatsapp_pin.size
                    pos: whatsapp_pin.pos 
        BoxLayout:
            orientation: 'horizontal'
            #rows:1
            #cols: 3
            Reqbutton:
                background_normal: 'redicon.png'
                background_down: 'darkredicon.png'
                #anchor_y: 'bottom'
                #anchor_x: 'left'
                text: 'Cancel'
                on_press: root.dismiss() 
            Labeller:
                width: 275
            Reqbutton:
                #anchor_y: 'bottom'
                #anchor_x: 'right'
                #size_hint: None, None
                #size: 50, 50
                background_normal: 'blueicon.png'  
                background_down: 'darkblueicon.png' 
                #background_normal: ''
                #background_down:
                text: 'Send'
                on_press: root.send_message()
                on_release: root.dismiss()
            #Labeller:
            #Labeller:


<MediaScreen>:
    name: "MediaScreen"
    id: boxerer
    grid: grid
    actress: actress
    BoxLayout:
        orientation: 'vertical' 
        ActionBar:
            background_color: utils.get_color_from_hex('#4169E1')#'#00BFFF')        
            #id: actress
            pos_hint: {'top':1}
            ActionView:
                use_separator: True
                id: actress
                #with 
                ActionPrevious:
                    with_previous: False
                    app_icon: "logo-white.png"
                ActionOverflow:          
                ClockLabel:
                ActionButton:
                    id: whatsapp
                    on_press: root.send_via_whatsapp()
