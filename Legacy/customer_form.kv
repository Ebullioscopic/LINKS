                text: 'ID'
                font_size: 20
                #size_hint_x: None#0.001
            CustomLabel2:#CustomLabel:
                text: 'Name'
                font_size: 20
                #size_hint_x: None
            CustomLabel2:#CustomLabel:
                text: 'City'
                font_size: 20
                #size_hint_x: None
            CustomLabel2:#CustomLabel:
                text: 'Phone'
                font_size: 20
                #size_hint_x: None
            CustLabel:
                #size_hint_x: None
            CustomerLabel2:               
                #size_hint_x: None 
            #Button:
            #    id: lol
            #    on_press: root.verify(self)    
        ScrollView:
            id: scroll
            do_scroll_x: False
            do_scroll_y: True
            scroll_wheel_distance: 250
            #smooth_scroll_end: 0.1
            GridLayout:
                id: grid  
                #orientation: "vertical"  
                cols: 6
                size_hint: 1, None
                size_hint_y: None
                height: self.minimum_height

<TableRow>:
    #firsttext: firsttext
    #secondtext: secondtext
    #thirdtext: thirdtext
    #fourthtext: fourthtext
    height: 1
    size_hint_y: None
    GridLayout:
        cols: 6
        #GridLayout:
            #cols: 6
        CustomerLabel:
            text: root.firsttext
        CustomLabel:
            text: root.secondtext
        CustomLabel:
            text: root.thirdtext
