                #    id: checker_button
                #    on_press: root.check_all_checkbutton()
                ActionButton:
                    id: close
                    #text: 'Close'
                    on_press: app.root.current = 'AdminDetails'
                    Image:
                        source: 'icons8-close-window.png'
                        allow_stretch: True
                        keep_ratio: True
                        size: close.size
                        pos: close.pos                    
